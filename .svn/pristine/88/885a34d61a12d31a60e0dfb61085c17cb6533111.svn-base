    #region MeasureRow
    public class MeasureRow
    {
        public MeasureRow(string name, string sValue, float fValue, string format, string unit, bool @string=false)
        {
            Name = name;
            Value = sValue;
            FValue = fValue;
            Format = format;
            Unit = unit;
            IsString = @string;
        }

        public string Name { get; set; }

        private string sValue;
        public string Value
        {
